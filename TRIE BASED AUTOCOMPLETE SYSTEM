class TrieNode:
    def __init__(self):
        self.children = {}
        self.is_end = False

class Trie:
    def __init__(self):
        self.root = TrieNode()

    def insert(self, word):
        node = self.root
        for ch in word:
            if ch not in node.children:
                node.children[ch] = TrieNode()
            node = node.children[ch]
        node.is_end = True

    def search_prefix(self, prefix):
        node = self.root
        for ch in prefix:
            if ch not in node.children:
                return None
            node = node.children[ch]
        return node

    def autocomplete(self, prefix):
        node = self.search_prefix(prefix)
        results = []
        if not node:
            return results

        def dfs(curr, path):
            if curr.is_end:
                results.append(prefix + path)
            for ch, nxt in curr.children.items():
                dfs(nxt, path + ch)

        dfs(node, "")
        return results

if __name__ == "__main__":
    trie = Trie()
    for word in ["apple", "app", "apex", "bat", "ball"]:
        trie.insert(word)
    print(trie.autocomplete("ap"))
